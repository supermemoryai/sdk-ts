{
  "dxt_version": "0.2",
  "name": "supermemory-mcp",
  "version": "4.6.0",
  "description": "The official MCP Server for the Supermemory API",
  "author": {
    "name": "Supermemory",
    "email": "dhravya@supermemory.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supermemoryai/sdk-ts.git"
  },
  "homepage": "https://github.com/supermemoryai/sdk-ts/tree/main/packages/mcp-server#readme",
  "documentation": "https://docs.supermemory.ai",
  "server": {
    "type": "node",
    "entry_point": "index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "SUPERMEMORY_API_KEY": "${user_config.SUPERMEMORY_API_KEY}"
      }
    }
  },
  "user_config": {
    "SUPERMEMORY_API_KEY": {
      "title": "api_key",
      "description": "",
      "required": true,
      "type": "string"
    }
  },
  "tools": [],
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
